{% block html_head_container %}

{% include 'html-header.twig' %}
	{% block head %}
	{% endblock %}
	</head>
{% endblock %}

	<body class="{{body_class}} {{post.post_name}}" data-template="base.twig">
		<a class="skip-link screen-reader-text sr-only" href="#content">{{ _e( 'Skip to content') }}</a>
		<header class="header">
			{% block header %}
				{% if se_active %}
					{% include 'partial/se-menu.twig' %}
				{% else %}
					{% include 'partial/sv-menu.twig' %}
				{% endif %}
			{% endblock %}
		</header>

		<main id="content" role="main" class="content-wrapper">
			{% if title %}<h1>{{title}}</h1>{% endif %}
			<div class="wrapper {{sidebar_class}}">
				{% block content %}
					Sorry, no content
				{% endblock %}
			</div>
		</main>
		{% if sidebar %}
			<aside class="layout-sidebar">
				{{sidebar}}
			</aside>
		{% endif %}

		{% block footer %}
			<footer id="footer">
				{% include 'footer.twig' %}
			</footer>
			{{ function('wp_footer') }}
		{% endblock %}
	</body>
</html>
